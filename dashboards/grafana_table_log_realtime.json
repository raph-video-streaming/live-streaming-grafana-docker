          "logsPerPage": 100,
          "logstore": "thamanyahcdn_v1",
          "powerSql": false,
          "query": "* | select\r\n  __time__ AS time,\r\n  client_ip,\r\n  request_time AS response_time,\r\n  refer_domain AS referrer,\r\n  method AS http_method,\r\n  uri AS request_url,\r\n  uri_param,\r\n  return_code AS http_status,\r\n  request_size AS request_bytes,\r\n  response_size AS response_bytes,\r\n  hit_info AS cache_status,\r\n  user_agent,\r\n  content_type AS file_type,\r\n  domain AS access_ip\r\nfrom log\r\nwhere\r\n\r\n  -- HTTP method filter\r\n  ('${http_method}' = '' OR method = '${http_method}')\r\n\r\n  -- Request URL filter\r\n  AND ('${request_url}' = '' OR uri LIKE CONCAT('%', '${request_url}', '%'))\r\n  -- Client IP filter\r\n  AND ('${client_ip}' = '' OR client_ip LIKE CONCAT('%', '${client_ip}', '%'))\r\n  -- Cache status filter\r\n  AND ('${cache_status}' = '' OR hit_info = '${cache_status}')\r\n\r\n  -- User agent filter\r\n  AND ('${user_agent}' = '' OR user_agent LIKE CONCAT('%', '${user_agent}', '%'))\r\n\r\n  -- Referrer filter\r\n  AND ('${referrer}' = '' OR refer_domain LIKE CONCAT('%', '${referrer}', '%'))\r\n\r\n\r\nlimit 100\r\n",
          "refId": "A",
          "totalLogs": 100,
          "type": "all",
          "xcol": "",
          "ycol": ""
        }
      ],
      "title": "Logs",
      "type": "table"
    }
